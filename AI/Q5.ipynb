      "source": [
        "def calculate_average(marks):\n",
        "    total = 0\n",
        "    count = 0\n",
        "    for m in marks:\n",
        "        total = total + m\n",
        "        count = count + 1\n",
        "\n",
        "    avg = total / count\n",
        "    print(\"Average marks: \",avg)\n",
        "    return avg\n",
        "\n",
        "marks = [82, 60, 76, 56]\n",
        "avg = calculate_average(marks)\n",
        "print(\"Average marks:\", avg)\n"
